{
  "name": "redom-mostjs",
  "version": "0.0.1",
  "description": "redom-mostjs experiment project",
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "node watch",
    "build-css": "stylus -w -u nib css/index.styl -c -o public/css/main.css",
    "build-js": "rollup -m -w -c -f iife js/index.js -o public/js/main.js",
    "uglify": "uglifyjs public/js/main.js -cm -o public/js/main.min.js",
    "start": "node server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@redom/store": "~1.0.0",
    "gulp-tabify": "0.0.2",
    "nib": "~1.1.2",
    "redom": "~3.18.0",
    "rimraf": "^3.0.2",
    "rollup": "~1.3.3",
    "rollup-plugin-buble": "~0.19.6",
    "rollup-plugin-butternut": "~0.1.0",
    "rollup-plugin-node-resolve": "~4.0.1",
    "stylus": "~0.54.5"
  },
  "dependencies": {
    "@most/core": "^1.5.0",
    "compression": "~1.7.3",
    "express": "~4.16.4",
    "gulp": "^4.0.2",
    "gulp-soften": "0.0.1"
  }
}
